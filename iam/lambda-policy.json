{
    "Version" : "2012-10-17",
    "Statement" : [
      {
        "Action" : [
          "s3:GetObject*",
          "s3:ListBucket"
        ],
        "Effect"   : "Allow",
        "Resource" : [
          "arn:aws:s3:::${aws_s3_bucket.bucket-test.id}",
          "arn:aws:s3:::${aws_s3_bucket.bucket-test.id}/*"
          ]
        },
        {
        "Action" : [
          "dynamodb:GetItem",
          "dynamodb:GetRecords",
          "dynamodb:PutItem"
          ],
          "Effect"   : "Allow",
          "Resource" : [
          "arn:aws:s3:::${aws_dynamodb_table.count_row_csv.arn}"
          ]
        }
    ]
  }
    